            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\Users\\u450930\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
